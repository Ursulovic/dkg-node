# Pinecone Configuration (required for RAG vector database functionality)
PINECONE_API_KEY=your_pinecone_api_key_here
PINECONE_INDEX=your_pinecone_index_name

# OpenAI Configuration (required for embeddings generation in RAG)
OPENAI_API_KEY=your_openai_api_key_here

# Google AI Configuration (required for Gemini-based article summarization)
GOOGLE_API_KEY=your_google_api_key_here

# LangSmith Observability (optional - enables tracing and monitoring of RAG operations)
# Get your API key from: https://smith.langchain.com/settings
LANGSMITH_TRACING=true
LANGSMITH_API_KEY=your_langsmith_api_key_here
LANGSMITH_PROJECT=plugin-bias-lens

# Tools
SERPAPI_API_KEY=your_serpapi_key
TAVILY_API_KEY=your_tavily_api_key

# BiasLens Knowledge Asset Configuration
PUBLISHER_NAME=BiasLens
BIASLENS_APP_URL=https://biaslens.ai
AGENT_VERSION=2.0
READ_COST_MULTIPLIER=2.0
TRAC_USD_RATE=0.5
